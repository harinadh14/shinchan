
<script>
function showimg() {
  var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4) {
     document.getElementById("psdiv").innerHTML = this.responseText;
    }
  };
  xhttp.open("GET", "http://<%= msg %>:2346/ps", true);
  xhttp.send();
}




function sendimgdata(){

let cntnrname= document.getElementById("cname").value;
let bsimgname= document.getElementById("osname").value;
 var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4) {
     document.getElementById("iddiv").innerHTML = this.responseText;
    }
  };
  xhttp.open("GET", "http://<%= msg %>:2346/imgid?cname="+cntnrname+"&"+"osname="+bsimgname , true);
  xhttp.send();


}








//function to create the host ajax 
function hostmmyfile(){

let portno= document.getElementById("portno").value;
let hostname= document.getElementById("hostname").value;
let filename= document.getElementById("filename").value;
let serverfilename = document.getElementById("serverfilename").value;
 var xhttp = new XMLHttpRequest();
  xhttp.onreadystatechange = function() {
    if (this.readyState == 4) {
     document.getElementById("hosturl").innerHTML = this.responseText;
    }
  };
  xhttp.open("get", "http://<%= msg %>:2346/hosting?portno="+ portno +"&hostname="+ hostname +"&filename="+ filename +"&serverfile=" + serverfilename, true);
  xhttp.send();


}



</script>

